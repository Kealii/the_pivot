<%= form_for @item, url: admin_items_path do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>

  <%= f.label :description %>
  <%= f.text_field :description %>

  <%= f.label :price %>
  <%= f.text_field :price%>

  <%= f.label :category %>
  <%= f.collection_select :categories, @categories, :id, :title ,{:multiple => true}%>

  <%= f.label :picture %>
  <%= f.file_field :picture%>

  <%= f.submit %>
<% end %>
