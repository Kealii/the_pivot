<%= form_for [:admin, @item], html: {multipart: true} do |f| %>
  <%= f.label 'title' %>
  <%= f.text_field 'title' %>
  <%= f.label 'description' %>
  <%= f.text_field 'description' %>
  <%= f.label 'price' %>
  <%= f.text_field 'price' %>
  <%= f.label :picture %>
  <%= f.file_field :picture%>

  <%= f.submit %>
<% end %>
